{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:jsx-a11y/recommended"
    ],
    "settings": {
        "react":{
            "version":"detect"
        }
    },
    "overrides": [
        {
            "files":["**/*.test.js","**/*.test.jsx"],
            "env":{
                "jest":true
            }
        }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "testing-library"
    ],
    "rules": {
        // "react/react-in-jsx-scope":"off",
        // "react/prop-types":"off",
        // "no-unused-vars":"off",
        "no-console": 1
        // "no-empty":"off"
    }
}
